{
    "name": "proner/phppimaco",
    "description": "Lib para geração de Etiquetas PIMACO",
    "keywords": ["PIMACO", "etiquetas","tags","hang tags"],
    "license": "MIT",
    "authors": [
        {
            "name": "Diogo Bemfica",
            "email": "diogo.fragabemfica@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Proner\\PhpPimaco\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "mpdf/mpdf": "^8.0",
        "endroid/qr-code": "^2.5|^3.0",
        "picqer/php-barcode-generator": "^2.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.3-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proner\\PhpPimacoTest\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
